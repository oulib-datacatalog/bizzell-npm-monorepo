  "main": "build/index.js",
  "scripts": {
    "precommit": "npm run lint && npm run test",
    "lint": "lint-staged",
    "test": "jest",
    "dev": "create-bizzell-app --dev",
    "build": "create-bizzell-app --build",
    "edit": "create-bizzell-app --edit"
  },
  "repository": {
    "type": "git",
