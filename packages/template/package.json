    "tslint-react": "3.6.0",
    "typescript": "3.1.3"
  },
  "dependencies": {
    "@bizzell/tempest": "^1.3.0",
    "classnames": "2.2.6",
    "font-awesome": "4.7.0",
    "froala-editor": "2.9.3",
