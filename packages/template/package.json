    "@types/react-router": "4.4.4",
    "@types/react-router-dom": "4.3.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "husky": "1.1.3",
    "jest": "24.7.1",
    "lint-staged": "8.1.0",
    "prettier": "1.14.3",
    "react-testing-library": "6.1.2",
