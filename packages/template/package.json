  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(\\.|/)test\\.tsx?$",
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/styleMock.ts"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx",
      "css"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 2
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint -c tslint.json"
    ],
    "*.{ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@bizzell/create-bizzell-app": "^1.3.0",
